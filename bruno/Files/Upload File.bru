meta {
  name: Upload File
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/v1/files/upload
  body: multipartForm
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:multipart-form {
  file: @file()
}

script:post-response {
  if (res.status === 201 && res.body.data && res.body.data.id) {
    bru.setVar("fileId", res.body.data.id);
  }
}
